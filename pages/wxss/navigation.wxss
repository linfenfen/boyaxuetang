/*底部导航*/
.navigationBar{
  display: flex;
  flex-direction: row;
  align-items: center;
  position: absolute;
  bottom:0;
  left:0;
  right:0;
  height:140rpx;
  background-color: #eeeeee;
  z-index:100;
  overflow: hidden;
}
.navigationBar .item{
  display: flex;
  flex-direction: column;
  align-items: center;
  flex:1;
}
.navigationBar .item image{
  width:76rpx;
  height:76rpx;
}
.navigationBar .item text{
  font-size:28rpx;
  font-weight:bold;
  color:#0a8c3b;
}
