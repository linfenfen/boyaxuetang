/*底部导航*/
.navigationBar{
  display: flex;
  flex-direction: row;
  align-items: center;
  position: absolute;
  bottom:0;
  left:0;
  right:0; 
  height:80rpx;
  background-color: #eeeeee;
  z-index:100;
  overflow: hidden;
}
.navigationBar .item{
  display: flex;
  flex-direction: column;
  align-items: center;
  flex:1;
}
.navigationBar .item image{
  width:50rpx;
  height:50rpx;
}
.navigationBar .item text{
  font-size:20rpx;
}
