/**app.wxss**/
::-webkit-scrollbar{
  	width:0px;
}
input[type='hidden']{
  width:0;
  height:0;
  line-height:0;
  min-height:0;
  visibility: hidden;
}
page{
  height:100%;
  overflow: hidden;
}
view,text{
  font-size:26rpx;
  font-family: Microsoft YaHei;
}
.container{
  position: absolute;
  top:0;
  left:0%;
  right:0;
  bottom:80rpx;
  overflow-y:scroll; 
}
